{
	"pages": [
		{
			"path": "pages/main/home/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/main/personal/index",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		}
	],
	"subPackages": [
		{
      "root": "pages/common",
			"name": "公共module",
      "pages": [
        {
          "path": "webview/index",
          "style": {
            "navigationBarTitleText": "webview"
          }
        },
				{
          "path": "detail/index",
          "style": {
            "navigationBarTitleText": "详情页"
          }
        }
      ]
    },
		
		{
      "root": "pages/user",
			"name": "用户module",
      "pages": [
        {
          "path": "login/index",
          "style": {
            "navigationBarTitleText": "登录"
          }
        },
				{
          "path": "register/index",
          "style": {
            "navigationBarTitleText": "注册"
          }
        }
      ]
    }
	],
	"preloadRule": {
    "pages/main/home/index": {
      "network": "all",
      "packages": ["pages/common"]
    },

		"pages/main/personal/index": {
      "network": "all",
      "packages": ["用户module"]
    }
  },
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
			{
        "pagePath": "pages/main/home/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/main/personal/index",
        "text": "个人中心"
      }
    ]
  }
}
